<p id="notice"><%= notice %></p>

<h1>Projects</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Link</th>
      <th>Owner</th>
      <th>Start Date</th>
      <th>End Date</th>
      <th>Description</th>
      <th>Cover</th>
      <th>Contribution ID</th>
      <th>Display Line ID</th>
      <th>Awarded ID</th>
      <th>Award ID</th>
      <th>Type ID</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.ProjectName %></td>
        <td><%= project.ProjectLink %></td>
        <td><%= project.ProjectOwner %></td>
        <td><%= project.ProjectStart %></td>
        <td><%= project.ProjectEnd %></td>
        <td><%= project.ProjectDescription %></td>
        <td><%= project.ProjectCover %></td>
        <td><%= project.ContributionID %></td>
        <td><%= project.DisplayLineID %></td>
        <td><%= project.AwardedID %></td>
        <td><%= project.AwardID %></td>
        <td><%= project.TypeID %></td>
        <td><%= link_to 'Show', project %></td>
        <td><%= link_to 'Edit', edit_project_path(project) %></td>
        <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project', new_project_path %>
<%= link_to 'Types', types_path %>
<%= link_to 'Contributions', contributions_path %>
<%= link_to 'Awards', awards_path %>
<%= link_to 'Types', types_path %>
<%= link_to 'Display Lines', display_lines_path %>
